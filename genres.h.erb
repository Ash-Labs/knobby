#define GENRE_COUNT <%= genres.count %>

const char* genres[GENRE_COUNT] = {
<% genres.each_with_index do |genre, i| -%>
  "<%= genre.name %>"<%= "," unless i == genres.size - 1 %>
<% end -%>
};

const char* playlists[GENRE_COUNT] = {
<% genres.each_with_index do |genre, i| -%>
  "<%= genre.id %>"<%= "," unless i == genres.size - 1 %>
<% end -%>
};
